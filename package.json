{
  "name": "akita-react-hooks-ts",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@datorama/akita": "^4.4.0",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.12",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "typescript": "3.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx} && eslint src/**/*.{js,jsx}",
    "prettier": "prettier \"**/*.{ts,tsx,scss,css,js,json,md}\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,scss,css,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "akitaCli": {
    "customFolderName": "true",
    "isAngular": false,
    "basePath": "./src/core/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@datorama/akita-cli": "^6.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-react-app": "^0.0.0",
    "tslint-react": "^4.0.0"
  }
}
