{
  "name": "akita-react-hooks-ts",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@datorama/akita": "^4.14.1",
    "@nitor-infotech/rxjs-use-selector": "^0.1.1",
    "@types/jest": "24.0.23",
    "@types/node": "14.14.33",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json src/**/*.{ts,tsx} && eslint src/**/*.{js,jsx}",
    "prettier": "prettier \"**/*.{ts,tsx,scss,css,js,json,md}\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,scss,css,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "akitaCli": {
    "customFolderName": "true",
    "isAngular": false,
    "basePath": "./src/core/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@datorama/akita-cli": "^6.0.3",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "tslint": "^5.20.1",
    "tslint-config-react-app": "^0.0.0",
    "tslint-react": "^4.1.0"
  }
}
