{
  "name": "hooks-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@datorama/akita": "^3.0.4",
    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "typescript": "3.3.3333"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' && eslint src/**/*.{js,jsx}",
    "prettier": "prettier \"**/*.{ts,scss,css,js,json,md}\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,scss,css,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "akitaCli": {
    "customFolderName": "true",
    "isAngular": false,
    "basePath": "./src/core/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@datorama/akita-cli": "^3.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react-hooks": "^1.5.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "tslint-config-react-app": "^0.0.0",
    "tslint-react": "^3.6.0"
  }
}
